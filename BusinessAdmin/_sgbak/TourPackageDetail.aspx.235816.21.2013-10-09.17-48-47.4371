    <script type="text/javascript">
        function suggestVehicleType() {
            alert("here");
            $('[id$=SelectTest]').each(function (index, value) {
                var projects = $.parseJSON($('[id$=VehicleTypeListHiddenField]').val());
               
                $(value).autocomplete({
                    minLength: 0,
                    source: projects,
                    focus: function (event, ui) {
                        $(value).val(ui.item.label);
                        return false;
                    },
