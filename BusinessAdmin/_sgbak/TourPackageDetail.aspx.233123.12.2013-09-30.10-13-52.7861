    <script type="text/javascript">
        function suggestVehicleType() {
            $(".gridTextBox").each(function (index, value) {
                var projects = $.parseJSON($('[id$=VehicleTypeListHiddenField]').val());
                $(value).autocomplete({
                    minLength: 0,
                    source: projects,
                    focus: function (event, ui) {
                        $(value).val(ui.item.label);
                        return false;
                    },
                    select: function (event, ui) {
                        $(value).val(ui.item.label);
                        $('[id$=SelectedVehicleTypeHiddenField]').val($('[id$=SelectedVehicleTypeHiddenField]').val()  + ui.item.value + ',');
                        return false;
                    }
                })
            .data("ui-autocomplete")._renderItem = function (ul, item) {
                return $("<li>")
            .append("<a>" + item.label + "<br/>" + "Max Pax :" + item.Capacity + "</a>")
            .appendTo(ul);
            };

            });
    }
    function suggestDetailVehicleType() {
        $(".gridDetailTextBox").each(function (index, value) {
            var projects = $.parseJSON($('[id$=VehicleTypeListHiddenField]').val());
            $(value).autocomplete({
                minLength: 0,
                source: projects,
                focus: function (event, ui) {
                    $(value).val(ui.item.label);
                    return false;
                },
                select: function (event, ui) {
                    $(value).val(ui.item.label);
                    $('[id$=SelectedDetailVehicleTypeHiddenField]').val($('[id$=SelectedDetailVehicleTypeHiddenField]').val()  + ui.item.value);
                    return false;
                }
            })
            .data("ui-autocomplete")._renderItem = function (ul, item) {
                return $("<li>")
            .append("<a>" + item.label + "<br/>" + "Max Pax :" + item.Capacity + "</a>")
            .appendTo(ul);
            };

        });
    }
    function ValidateBasicData() {

        var Adult = $('[id$=NoOfAdultListBox]').val()
        var Child = $('[id$=NoOfChildListBox]').val()
        var AdultCount = 0;
        var ChildCount = 0;
        var AdultRoomList = new Array();
        var ChildRoomList = new Array();
        var TotalRoom = $('[id$=RoomNumberDropDownList]').val();
        if (Child > 0) {
            //do nothing
        }
        else {
            Child = 0;
        }
        
        $('#RoomList select.gridSelect').each(function (index, value) {
            var t = $(value).val();
            t = parseInt(t);
            AdultRoomList[index] = t;
            AdultCount = AdultCount + t;

        });
        
        $('#RoomList select.gridRoomSelect').each(function (index, value) {
            var t = $(value).val();
            t = parseInt(t);
            ChildRoomList[index] = t;
            ChildCount = ChildCount + t;

        });
        for (i = 0; i < TotalRoom; i++)
        {
           var  TotalAdult = AdultRoomList[i];
            var TotalChild = ChildRoomList[i];
            var TotalPerRoom = TotalAdult + TotalChild;
            if (TotalPerRoom > 4)
            {
                $.Zebra_Dialog('Only 4 Person Per Room');
                return false;
            }
            if (TotalAdult == 0) {
                $.Zebra_Dialog('1 Adult Must Be Exist In Each Room');
                return false;
            }
        }
                   

        $('[id$=SelectedRoomListHiddenField]').val("");
        $('[id$=PaxListForVehicleHiddenField]').val("");
            if ($('[id$=ArrivalDateTextBox]').val() == "") {
                $.Zebra_Dialog('Arrival Date Is Required');
                return false;
            }
            else if ($('[id$=AccommodationTypeDropDownList]').val() == "0") {
                $.Zebra_Dialog('Accomadation Type Is Required');
                return false;
            }
            else if ($('[id$=RoomPlanDropDownList]').val() == "0") {
                $.Zebra_Dialog('You Must Select Room Plan');
                return false;
            }
            else if ($('[id$=RoomNumberDropDownList]').val() == "0") {
                $.Zebra_Dialog('Provide Number Of Room  Required');
                return false;
            }
            else if ($('[id$=NoOfAdultListBox]').val() == "0") {
                $.Zebra_Dialog('Number Of Adult Cannot Be 0');
                return false;
            }          
           
           
            else
                if (AdultCount > Adult) {
                    $.Zebra_Dialog('Total Adult(s) in Room is Greater than total adult Arriving');
                    return false;
                }
                else if (AdultCount != Adult) {
                    $.Zebra_Dialog('Total Adult(s) In Room Did Not Match With Total Arrival Adult(s)');
                    return false;
                }
                else if (ChildCount != Child) {
                    $.Zebra_Dialog('Total Childern(s) In Room Did Not Match With Arrival Childern(s)');
                    return false;
                }
                else if (ChildCount > Child) {
                    $.Zebra_Dialog('Total Childs in Room is Greater than total Child');
                    return false;
                }
             else {
                $('#RoomList select.gridSelect').each(function (index, value) {
                    $('[id$=SelectedRoomListAdultHiddenField]').val($('[id$=SelectedRoomListAdultHiddenField]').val() + $(value).val() + ',');
                });
                $('#RoomList select.gridRoomSelect').each(function (index, value) {
                    $('[id$=SelectedRoomListChildHiddenField]').val($('[id$=SelectedRoomListChildHiddenField]').val() + $(value).val() + ',');
                });
                $('#VehicleList select').each(function (index, value) {
                    $('[id$=PaxListForVehicleHiddenField]').val($('[id$=PaxListForVehicleHiddenField]').val() + $(value).val() + ',');
                });
              
                return true;
            }
        }
        function ValidateDetailData() {
            
            var AdultCount = 0;
            var ChildCount = 0;
            var TotalRoom = $('[id$=DetailRoomNumberDropDownList]').val()           
            var Child = $('[id$=DetailChildNumberListBox]').val()
            var Adult = $('[id$=DetailAdultNumberListBox]').val()

            if (Child == null) {
                Child = 0;
            }
            if (Adult == null) {
                Adult = 0;
            }
            if (TotalRoom == null) {
                TotalRoom = 0;
            }
            var AdultRoomList = new Array();
            var ChildRoomList = new Array();
            

            $('select.gridDetailSelect').each(function (index, value) {
                var t = $(value).val();
                t = parseInt(t);
                AdultRoomList[index] = t;
                AdultCount = AdultCount + t;
            })

            $('#DetailRoomNumber select.gridSelect').each(function (index, value) {
                var t = $(value).val();
                t = parseInt(t);
                ChildRoomList[index] = t;
                ChildCount = ChildCount + t;
            })

            for (i = 0; i < TotalRoom; i++)
            {
                var TotalAdult = AdultRoomList[i];
                var TotalChild = ChildRoomList[i];
                var TotalPerRoom = TotalAdult + TotalChild;
                if (TotalPerRoom > 4)
                {
                    $.Zebra_Dialog('Only 4 Person Per Room');
                    return false;
                }
                if (TotalAdult == 0) {
                    $.Zebra_Dialog('1 Adult Must Be Exist In Each Room');
                    return false;
                }
            }


            if ($('[id$=DetailArrivalDateTextBox]').val() == "") {
                $.Zebra_Dialog('Arrival Date Is Required');
                return false;
            }
            else if ($('[id$=DetailAdultNumberListBox]').val() == "0") {
                $.Zebra_Dialog('Number Of Adult Cannot Be 0');
                return false;
            }
            else if ($('[id$=DetailRoomPlanDropDownList]').val() == "0") {
                $.Zebra_Dialog('You Must Select Room Plan');
                return false;
            }
            else if ($('[id$=DetailRoomNumberDropDownList]').val() == "0") {
                $.Zebra_Dialog('Provide Number Of Room  Required');
                return false;
            }
            else if (AdultCount > Adult) {
                $.Zebra_Dialog('Adult in the room are greater than total adult arriving');
                return false;
            }
            else if (ChildCount > Child) {
                $.Zebra_Dialog('Childern in the room are greater than total childern arriving');
                return false;
            }
            else
                if (AdultCount != Adult) {
                    $.Zebra_Dialog('Adults in Room Did Not Matched With Adult Arrivial');
                    return false;
                }
                else if (ChildCount != Child) {
                    $.Zebra_Dialog('Childs in Room Did Not Matched With Childs Arrivial');
                return false;
                }
            return true;
        }
        
    
    </script>
    <script type="text/javascript">

        $(document).ready(function () {
    
        $("#PackageDetail").fancybox({
            'titleShow'     : true,
            'transitionIn'  : 'elastic',
            'transitionOut': 'elastic',
        });
        });
        
        function maintainImageGallery() {
            $(".fancybox").fancybox({
                beforeShow: function () {
                    if (this.title) {

                    }
                },
                buttons: {},
                openEffect: 'fade',
                closeEffect: 'fade',
                closeBtn: true,
                type: 'image'
            });
            var totalImages = jQuery("#gallery > li").length;
            var imageWidth = jQuery("#gallery > li:first").outerWidth(true);
            var totalWidth = imageWidth * totalImages;
            var visibleImages = Math.round(jQuery("#gallery-wrap").width() / imageWidth);
            var visibleWidth = visibleImages * imageWidth;
            var stopPosition = (visibleWidth - totalWidth);
            jQuery("#gallery").width(totalWidth);
            jQuery("#gallery-prev").click(function () {

                if (jQuery("#gallery").position().left < 0 && !jQuery("#gallery").is(":animated")) {
                    jQuery("#gallery").animate({ left: "+=" + imageWidth + "px" });
                }
                return false;
            });

            jQuery("#gallery-next").click(function () {
                if (jQuery("#gallery").position().left > stopPosition && !jQuery("#gallery").is(":animated")) {
                    jQuery("#gallery").animate({ left: "-=" + imageWidth + "px" });
                }
                return false;
            });


        }
    
    </script>
